
@{
    ViewData["Title"] = "Index";
}

<form asp-action="UploadImage"
      asp-controller="Image"
      method="post"
      enctype="multipart/form-data">
    <input type="file" id="file1"
           name="file1"
           multiple="multiple" />
    <button type="submit">Upload File(s)</button>
</form>
<h1>@ViewBag.Message</h1>
